<div>
    {{#with person}}
    <div>
        <h1> {{name}}</h1>
        <div> {{age}}</div>
        <div> {{gender}}</div>
        <address> {{#with address}}
            {{street}}, {{city}}, {{state}}, {{zip}}, {{country}}
            {{/with}}
        </address>
        <hr>
        <br>
    </div>

    <br>

    {{>title big=true data='Education'}}

    <div>
        {{#each education}}
        <div> {{degree}} </div>
        <div> {{major}} </div>
        <div> {{university}} </div>
        {{/each}}
        <hr>
        <br>
    </div>

    <br>

    {{>title big=true data='WorkExperience'}}

    <div>
        {{#each workExperience}}
        <div> {{company}} </div>
        <div> {{title}} </div>
        <div> {{startDate}} - {{endDate}} </div>

        <div>
            <ul>
                {{#each responsibilities}}
                <li> {{this}} </li>
                {{/each}}
            </ul>
        </div>

        <div> Founded: {{year_founded}}</div>
        <div> Industry: {{industry}}</div>
        <br>

        <div>
            {{#each employees}}
            <div> <b> {{name}} </b></div>
            <div> {{position}}</div>
            <div> {{department}}</div>
            <ul>
                <li>
                    <div>
                        {{>title big=true data='Projects:'}}
                        <div>

                            {{#each projects}}
                            <div> <b> {{name}} </b> </div>
                            <div> {{description}} </div>
                            <div> {{status}} </div>
                            <ul>
                                {{#each teams}}
                                <li> <b> {{team_name}} </b> </li>
                                <div>
                                    <em>
                                        {{#with team_leader}}
                                        <div> <b> {{name}} </b> </div>
                                        <div> {{title}}</div>
                                        <div>
                                            <address>{{email}} </address>
                                            </address>
                                        </div>
                                        {{/with}}
                                    </em>
                                </div>
                                <br>
                                <br>

                                <li>
                                    {{#each team_members}}
                                    <div> <strong> Team members</strong></div>
                                    <div> <b>{{name}} </b> </div>
                                    <div> {{title}}</div>
                                    <div>
                                        <address>{{email}} </address>
                                    </div>
                                    <div> {{skills}}</div>

                                </li>

                                <br>
                                <br>

                                <div>
                                    {{#each projects}}
                                    <div> <b> {{name}} </b> </div>
                                    <div> {{description}}</div>
                                    <div> {{technologies}}</div>

                                    <br>

                                    <ul>
                                        <li> {{team_members.[0].name}} </li>
                                        <div> {{team_members.[0].title}} </div>
                                        <li> {{team_members.[1].name}} </li>
                                        <div> {{team_members.[1].title}} </div>
                                    </ul>
                                    {{/each}}
                                </div>
                                {{/each}}
                            </ul>
                        </div>
                        {{/each}}
                        {{/each}}
                </li>
            </ul>
        </div>
        {{/each}}
    </div>
    {{/each}}
    <hr>
    <br>
</div>
<br>
{{/with}}
</div>